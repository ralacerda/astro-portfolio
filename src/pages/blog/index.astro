---
import { getCollection } from "astro:content";
import Layout from "@/layouts/Layout.astro";

const blogEntries = await getCollection(
  "articles",
  ({ data }) => data.isDraft != true
);
---

<Layout pageName="">
  {
    blogEntries.map((item) => (
      <li>
        <a href={`/blog/${item.slug}`}>{item.data.title}</a>
      </li>
    ))
  }
</Layout>
