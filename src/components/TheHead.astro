---
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!-- Primary Meta Tags -->
<meta name="viewport" content="width=device-width, initial-scale=1" />
<meta charset="UTF-8" />
<title>{title}</title>
<meta name="title" content={title} />
<meta
  name="description"
  content="I'm a biologist changing careers into Front-end development.
      Looking for a learning opportunity to evolve as a developer. "
/>
