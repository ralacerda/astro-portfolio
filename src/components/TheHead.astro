---
interface Props {
  pageName: string;
}

const { pageName } = Astro.props;
const title = `Renato Lacerda | ${pageName}`;
---

<!-- Primary Meta Tags -->
<title>{title}</title>
<meta name="title" content={title} />
<meta
  name="description"
  content="I'm a biologist changing careers into Front-end development.
      Looking for a learning opportunity to evolve as a developer. "
/>
